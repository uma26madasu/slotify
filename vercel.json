{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "public",
    "framework": null,
    "installCommand": "npm install",
    "ignoreCommand": "git diff --quiet HEAD^ HEAD ./apps/web",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}