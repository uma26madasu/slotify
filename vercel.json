{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "public",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/apps/api/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}